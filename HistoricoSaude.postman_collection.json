											"pm.environment.set(\"usuario_medico_id\", medico_id);",
											"pm.environment.set(\"usuario_medico2_id\", medico2_id);",
											"pm.environment.set(\"usuario_enfermeiro_id\", enfermeiro_id);",
											"pm.environment.set(\"usuario_paciente_id\", paciente_id);"
